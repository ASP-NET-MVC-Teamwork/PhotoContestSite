@using Microsoft.AspNet.Identity
@model PhotoContest.Web.ViewModels.PictureViewModel

<div class="panel panel-body panel-default">
    <h3>@Model.Title</h3>
    <img src="@Url.Content(@Model.Url)" alt="" class="img-thumbnail" width="304" height="236"/>
</div>
@Html.ActionLink("Edit", "Edit", "AdminPictures", new { id = Model.PictureId, }, new { @class = "btn btn-default" })
<a href="#" class="deleteLink btn btn-danger" id="@Model.PictureId">delete</a>